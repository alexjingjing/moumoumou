.container {
  display: flex;
  flex-direction: column;
  padding: 50rpx 0;
}

.log-item {
  display: flex;
  flex-direction: row;
  justify-content: center;
  height: 100rpx;
}

.avatar {
  width: 80rpx;
  height: 80rpx;
}

.nickname {
  width: 180rpx;
  margin-left: 40rpx;
  margin-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
